{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "outDir": "./dist",
    "baseUrl": ".",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "typeRoots": ["./node_modules/@types"],
    "paths": {
      "@models/*":["./src/memory/models/*"],
      "@core/*":["./src/core/*"],
      "@memory/*":["./src/memory/*"],
      "@routers/*":["./src/routers/*"]
    }
  },
  "include": [
    "./src/**/*"
  ]
}
